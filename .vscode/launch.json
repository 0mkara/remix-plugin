  "version": "0.2.0",
  "configurations": [
    {
      "name": "Test",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["test"],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole"
    },
    {
      "name": "Vscode Example",
      "request": "launch",
      "type": "pwa-extensionHost",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/dist/examples/example/engine/vscode"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/examples/example/engine/vscode"
      ],
    }
  ]
}