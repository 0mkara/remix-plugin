{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Test",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["test"],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole"
    },
    {
      "name": "Vscode Example",
      "request": "launch",
      "type": "extensionHost",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/dist/examples/example/engine/vscode"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/examples/example/engine/vscode"
      ],
    },
    
    {
      "name": "Vscode Example 2",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/dist/examples/example/engine/vscode"
      ],
      "trace": "false",
      "internalConsoleOptions": "openOnFirstSessionStart",
      "outFiles": [
        "${workspaceFolder}/dist/examples/example/engine/vscode"
      ]
    }
  ]
}